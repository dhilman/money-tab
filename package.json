{
  "name": "moneytab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write --log-level warn .",
    "format:check": "prettier --check --log-level warn .",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run format:check",
    "test": "vitest",
    "test:e2e": "vitest test/",
    "bot:setup": "bun ./scripts/bot_setup.ts",
    "tunnel": "cloudflared tunnel run local",
    "db:run:main": "turso dev --port 5440 --db-file ./data/db/local/1/main.db",
    "db:run:monitor": "turso dev --port 5441 --db-file ./data/db/local/1/main.db",
    "db:run": "concurrently \"pnpm run db:run:main\" \"pnpm run db:run:monitor\"",
    "db:check": "dotenv drizzle-kit check:sqlite",
    "db:studio:main": "drizzle-kit studio --config ./migrations/main.config.ts",
    "db:studio:monitor": "drizzle-kit studio --config ./migrations/monitor.config.ts",
    "db:gen:main": "bun drizzle-kit generate --config=./migrations/main.config.ts",
    "db:gen:monitor": "bun drizzle-kit generate --config=./migrations/monitor.config.ts",
    "db:migrate": "bun run ./scripts/db_migrate.ts",
    "i18n:types": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/@types/resources.d.ts",
    "i18n:extract": "i18next 'src/**/*.{ts,tsx}'",
    "i18n:run": "pnpm run i18n:extract && pnpm run i18n:types",
    "run:staging": "bun run --env-file=./.envs/.env.staging ..."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.569.0",
    "@grammyjs/parse-mode": "^1.9.0",
    "@libsql/client": "^0.17.0",
    "@lottiefiles/lottie-player": "^2.0.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-table": "^8.16.0",
    "@tonconnect/ui-react": "^2.0.5",
    "@trpc/client": "^11.8.1",
    "@trpc/next": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@upstash/qstash": "^2.4.3",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "cookie": "^0.6.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.10",
    "drizzle-orm": "^0.45.1",
    "fuse.js": "^7.0.0",
    "grammy": "^1.23.0",
    "i18next": "^23.11.1",
    "i18next-http-backend": "^2.5.0",
    "jose": "^5.2.4",
    "lucide-react": "^0.424.0",
    "marked": "^12.0.1",
    "mysql2": "^3.9.1",
    "next": "^15.5.9",
    "next-themes": "^0.4.6",
    "pino": "^8.20.0",
    "react": "19.2.3",
    "react-day-picker": "^8.10.1",
    "react-dom": "19.2.3",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.0",
    "recharts": "^2.12.2",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "superjson": "^1.13.3",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "use-debounce": "^10.0.0",
    "vaul": "^0.9.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@inquirer/prompts": "^5.0.4",
    "@next/eslint-plugin-next": "^16.1.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.13",
    "@types/bun": "^1.0.8",
    "@types/cookie": "^0.6.0",
    "@types/eslint": "^8.56.2",
    "@types/node": "25.0.5",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@types/telegram-web-app": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "i18next-parser": "^8.13.0",
    "i18next-resources-for-ts": "^1.5.0",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.38",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "vitest": "^1.6.0"
  }
}
